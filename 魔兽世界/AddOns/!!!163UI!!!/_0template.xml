<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">

	<!--	NOTES
		"OnLoad" script is triggered before the time CreateFrame returns.
		GAMETOOLTIP must have a name. ([[name = "#NAME"]] in XML or [[CreateFrame("GAMETOOLTIP", "#NAME#", ..)]] in lua)
		GAMETOOLTIP must have two FontStrings named "#NAME#TextLeft1" and "#NAME#TextRight1" before "OnLoad" script returns.
		So
		GAMETOOLTIP must be defined in xml or created in lua using template.
		"#NAME#TextLeft1" and "#NAME#TextRight1" are defined in xml.
		textLeft1Font, textRight1Font, textLeft2Font, textRight2Font are not necessary. The are only used in SharedTooltipTemplate.lua
	-->
	<GameTooltip name="__CoreTooltipTemplate" virtual="true">
		<Layers>
			<Layer level="ARTWORK">
				<FontString name="$parentTextLeft1" parentKey="TextLeft1" inherits="GameTooltipHeaderText" />
				<FontString name="$parentTextRight1" parentKey="TextRight1" inherits="GameTooltipHeaderText" justifyH="LEFT">
					<Anchors>
						<Anchor point="RIGHT" relativeKey="$parent.TextLeft1" relativePoint="LEFT" x="40" y="0"/>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
	</GameTooltip>


